{
  "root": true,
  "extends": "eslint-config-egg",
  "rules": {
    "jsdoc/require-param-description": 0,
    "jsdoc/require-returns-description": 0,
    "jsdoc/check-tag-names": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "prefer-const": 0,
    "space-before-function-paren": 0,
    "array-bracket-spacing": 0,
    "no-unused-vars": 1
  }
}
